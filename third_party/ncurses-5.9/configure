$as_echo "no" >&6; }
		{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: did not find library $PKG_CONFIG_LIBDIR" >&5
$as_echo "$as_me: WARNING: did not find library $PKG_CONFIG_LIBDIR" >&2;}
		enable_pc_files=no
	fi
fi


