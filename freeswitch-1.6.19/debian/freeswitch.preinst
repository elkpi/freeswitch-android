#!/bin/sh
set -e

case "$1" in
  install|upgrade)
    if [ -d /opt/freeswitch ]; then
      echo "It looks like you have FreeSWITCH installed manually." >&2
    fi
    ;;
  abort-upgrade)
    ;;
  *)
    echo "preinst called with unknown argument \`$1'" >&2
    exit 1
    ;;
esac

#DEBHELPER#
exit 0
